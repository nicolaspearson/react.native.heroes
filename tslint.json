{
	"extends": ["tslint:recommended", "tslint-config-airbnb", "tslint-react"],
	"rules": {
		"no-any": true,
		"prefer-array-literal": [true, { "allow-type-parameters": true }],
		"indent": [true, "tabs"],
		"ter-indent": [true, "tab"],
		"trailing-comma": false,
		"variable-name": [
			true,
			"ban-keywords",
			"check-format",
			"allow-pascal-case",
			"allow-leading-underscore"
		]
	}
}
